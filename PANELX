/* REXX */
/*ISPEXEC DISPLAY*/
/* ARG OPTIONS */
ADDRESS ISPEXEC
"LIBDEF ISPPLIB DATASET ID('D.SYSTEM.A09.PANEL')" /* DYNAMICALLY ALLOCATES PANEL LIBRARY */
IF LENGTH(ZSEL)<>0 THEN DO FOREVER
CALL CLEAR
"DISPLAY PANEL(PANEL1)"
IF ZCMD = "CANCEL" THEN EXIT 4
IF RC > 0 THEN EXIT 4
IF RC = 0 THEN
  IF ZSEL <> NULL THEN DO
    IF ZSEL = "SMSADD" THEN DO WHILE ZSEL <> "0"
    CALL CLEAR
    "DISPLAY PANEL(SMSADD)"
    END
    IF ZSEL = "SMSVOL" THEN DO WHILE ZSEL <> "0"
    CALL CLEAR
    "DISPLAY PANEL(SMSVOL)"
    END
    IF ZSEL = "SMSTAT" THEN DO WHILE ST <> "0"
    CALL CLEAR
    "DISPLAY PANEL(SMSTAT)"
    END
  END
END
 
/* DISPLAY PANEL
IF ZSEL = "SMSD" THEN DO FOREVER
    "DISPLAY PANEL(SMSD)"
IF ZCMD = "CANCEL" THEN EXIT 4
IF RC = 0 THEN
  IF ZSEL <> NULL THEN LEAVE
IF RC > 0 THEN EXIT 4
END
 
/* ADD/REMOVE PANEL */
IF ZSEL = "SMSADD" THEN DO FOREVER
    "DISPLAY PANEL("ZSEL")"
IF ZCMD = "CANCEL" THEN EXIT 4
IF RC = 0 THEN
  IF ZSEL <> NULL THEN LEAVE
IF RC > 0 THEN EXIT 4
END
*/
 /* IF (ZSEL ="EXIT" OR ZCMD ="EXIT") THEN ST ="0"*/
"ISPEXEC LIBDEF ISPPLIB"
//*IF ZSEL='SMSD' THEN*//
CLEAR:
ARGS=''
ZSEL=''
ZCMD=''
ST=''
SYST=''
DISK=''
RETURN
