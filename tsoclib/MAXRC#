/* REXX */
CALL ISPPRXVP 'I'
RC=ISFCALLS('ON')
ADDRESS SDSF "ISFEXEC ST"
FIXEDFIELD = WORD(ISFCOLS,1)
MAXRC = ''
MAXPOS = 0
IF OPER = 'ADD' THEN COMPNAME = 'A09INIT'
IF OPER = 'DEL' THEN COMPNAME = 'A09REFOR'
DO IX=1 TO ISFROWS
IF JNAME.IX = COMPNAME THEN DO
  DO JX=1 TO WORDS(ISFCOLS)
    IF POS.IX > MAXPOS THEN DO
      MAXPOS = POS.IX
      MAXRC = SUBSTR(RETCODE.IX,4,4)
    END
  END
END
END
RC=ISFCALLS('OFF')
CALL ISPPRXVP 'T'
